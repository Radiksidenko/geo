{
  "compilerOptions": {
    "target": "es6",
    "module": "System",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "include": [
    "assets"
  ],
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
